  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "now-build": "npm run build",
    "format": "prettier --write update-today.js meta-image.js favicon.js src/**/*.{js,jsx,css} data/*.json",
    "favicon": "node favicon.js",
    "meta-image": "node meta-image",
    "update": "npm run favicon && npm run meta-image && git add . && git commit -m 'Update colour' && git push",
    "build-cli": "babel update-today.js src/utils/* --out-dir built",
    "develop-cli": "babel update-today.js src/utils/* --watch --out-dir built",
    "cli": "node built/update-today.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
