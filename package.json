    "babel-preset-gatsby": "^0.1.11",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "react-test-renderer": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
