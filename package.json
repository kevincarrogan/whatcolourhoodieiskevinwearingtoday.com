    "gatsby-plugin-sharp": "^2.0.34",
    "gatsby-source-filesystem": "^2.0.29",
    "gatsby-transformer-json": "^2.1.8",
    "gatsby-transformer-sharp": "^2.1.13",
    "hex-rgb": "^4.0.0",
    "ink": "^2.1.1",
    "ink-select-input": "^3.1.1",
